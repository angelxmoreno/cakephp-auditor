{
    "name": "angelxmoreno/cakephp-auditor",
    "description": "Auditor plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Angel S. Moreno",
            "homepage": "https://www.linkedin.com/in/angelxmoreno/",
            "role": "Author"
        }
    ],
    "support": {
        "issues": "https://github.com/angelxmoreno/cakephp-auditor/issues",
        "source": "https://github.com/angelxmoreno/cakephp-auditor"
    },
    "require": {
        "cakephp/cakephp": "^3.4",
        "composer/installers": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Auditor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Auditor\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "extra": {
        "installer-name": "Auditor"
    }
}
